import java.util.Scanner;

public class RightTriangleChar {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String symbol = sc.next();
        int max = sc.nextInt();
        
        for (int i = 1; i <= max; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print(symbol + " ");
            }
            System.out.println();
        }
        sc.close();
    }
}
